/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=x);
    Or(a=x, b=in[2], out=y);
    Or(a=y, b=in[3], out=z);
    Or(a=z, b=in[4], out=l);
    Or(a=l, b=in[5], out=m);
    Or(a=m, b=in[6], out=n);
    Or(a=n, b=in[7], out=out);
}